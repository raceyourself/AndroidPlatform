<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#ffa8a8aa"
            />
            <corners
                android:radius="4dp" />
        </shape>
    </item>
    <item android:state_focused="true" >
        <shape>
            <solid
                android:color="#ffa8a8aa"
            />
            <corners
                android:radius="4dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#ffd1d2d4"
            />
            <corners
                android:radius="4dp" />
        </shape>
    </item>
</selector>
