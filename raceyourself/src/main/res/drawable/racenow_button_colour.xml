<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="false" >
        <shape>
            <solid
                android:color="#BBBBBB"/>
            <corners
                android:radius="10dp"/>
        </shape>
    </item>
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#1C5C1E"
            />
            <corners
                android:radius="10dp" />
        </shape>
    </item>
    <item android:state_focused="true" >
        <shape>
            <solid
                android:color="#277629"
            />
            <corners
                android:radius="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#269B47"
            />
            <corners
                android:radius="10dp" />
        </shape>
    </item>
</selector>
